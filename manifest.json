{
	"manifest_version": 3,
	"name": "WebScreenExtension",
	"version": "1.0",
	"icons": {
		"16": "icon.png",
		"48": "icon.png",
		"128": "icon.png"
	},
	"permissions": [
		"activeTab"
	],
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_title": "Click Me",
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"optional_host_permissions": [
		"*://*/*"
	]
}
